{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": "./test/.+\\.e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["jest-extended"],
  "coverageDirectory": "./coverage/e2e",
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/{!((*.spec)|main),}.ts"]
}
